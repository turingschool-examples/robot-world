<h2>Add New Robot</h2>
<div class="robot">
  <form action="/robots" method="post">
    <label>Name</label>
    <input type="text" name='robot[name]'/><br/>
    <label>City</label>
    <input type="text" name='robot[city]'></input><br/>
    <label>State</label>
    <input type="text" name='robot[state]'/><br/>
    <label>Department</label>
    <input type="text" name='robot[department]'/><br/>
    <input type='submit'/>
  </form>
</div>
